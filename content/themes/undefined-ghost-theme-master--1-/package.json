  "version": "2.6.1",
  "engines": {
      "ghost": ">=3.11.0",
      "ghost-api": "v3"
  },
  "author": {
      "name": "Curiositry",
      "email": "isa@automathic.org",
      "url": "http://curiositry.com"
  },
  "config": {
      "posts_per_page": 5,
      "image_sizes": {
